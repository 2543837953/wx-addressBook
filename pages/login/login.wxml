<van-cell-group>
    <van-field
            model:value="{{ phone }}"
            placeholder="请输入手机号"
            border="{{ false }}"
            label="手机号"
    />
    <van-field
            model:value="{{ password }}"
            type="password"
            label="密码"
            placeholder="请输入密码"
            border="{{ false }}"
    />
</van-cell-group>
<view class="btn">
    <van-button round type="primary" block bind:click="getLogin">登录</van-button>
</view>



